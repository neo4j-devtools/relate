{
    "name": "@relate/cli",
    "description": "CLI tool for interacting with the Neo4j platform.",
    "version": "1.0.2-alpha.0",
    "author": "Neo4j Inc.",
    "license": "GPL-3.0",
    "homepage": "https://github.com/neo-technology/relate#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/neo-technology/relate.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "bin": {
        "relate": "./bin/run"
    },
    "dependencies": {
        "@nestjs/common": "7.0.8",
        "@nestjs/core": "7.0.8",
        "@oclif/command": "1.5.19",
        "@oclif/config": "1.13.3",
        "@oclif/plugin-autocomplete": "0.1.5",
        "@oclif/plugin-help": "2.2.3",
        "@relate/common": "^1.0.2-alpha.0",
        "@relate/types": "^1.0.2-alpha.0",
        "cli-ux": "5.4.5",
        "cross-env": "7.0.2",
        "enquirer": "2.3.4",
        "fs-extra": "9.0.0",
        "graphql": "15.1.0",
        "lodash": "4.17.19",
        "node-fetch": "2.6.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.5",
        "tslib": "1.10.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "1.22.2",
        "@oclif/test": "1.2.5",
        "@types/fs-extra": "8.1.0",
        "@types/jest": "25.1.0",
        "@types/lodash": "4.14.149",
        "@types/node": "10.17.13",
        "@types/node-fetch": "2.5.7",
        "chai": "4.2.0",
        "globby": "10.0.2",
        "jest": "25.4.0",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.1",
        "ts-jest": "25.4.0",
        "ts-node": "8.6.2",
        "typescript": "3.7.5"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "keywords": [
        "oclif"
    ],
    "oclif": {
        "commands": "./dist/commands",
        "topics": {
            "app": {
                "description": "Manage Graph Apps"
            },
            "db": {
                "description": "Manage Neo4j Databases"
            },
            "dbms": {
                "description": "Manage Neo4j DBMSs"
            },
            "environment": {
                "description": "Manage relate environments"
            },
            "extension": {
                "description": "Manage relate extensions"
            },
            "project": {
                "description": "Manage relate projects"
            }
        },
        "hooks": {
            "command_not_found": "./dist/hooks/command-not-found"
        },
        "bin": "relate",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-autocomplete"
        ]
    },
    "scripts": {
        "dev-cli": "cross-env NODE_PATH=./node_modules oclif-dev",
        "prepare": "npm run build",
        "postpack": "rimraf oclif.manifest.json",
        "prepack": "run-s build:clean build:manifest build:readme",
        "build": "npm run prepack",
        "build:clean": "run-s clean build:tsc",
        "build:tsc": "tsc -b",
        "build:manifest": "node -e '' | npm run dev-cli -- manifest",
        "build:readme": "npm run dev-cli -- readme --multi --dir=./docs",
        "clean": "rimraf dist",
        "test": "jest",
        "add:readme": "git add README.md",
        "version": "run-s build:readme add:readme",
        "watch": "tsc -w"
    }
}
