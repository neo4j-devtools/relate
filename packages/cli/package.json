{
    "name": "@relate/cli",
    "description": "CLI tool for interacting with the Neo4j platform.",
    "version": "1.0.2-alpha.11",
    "author": "Neo4j Inc.",
    "license": "GPL-3.0",
    "homepage": "https://github.com/neo4j-devtools/relate#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/neo4j-devtools/relate.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "bin": {
        "relate": "./bin/run"
    },
    "dependencies": {
        "@nestjs/common": "7.0.8",
        "@nestjs/core": "7.0.8",
        "@oclif/command": "1.8.0",
        "@oclif/config": "1.17.0",
        "@oclif/plugin-autocomplete": "0.2.0",
        "@oclif/plugin-help": "3.2.0",
        "@relate/common": "^1.0.2-alpha.11",
        "@relate/types": "^1.0.2-alpha.9",
        "cli-ux": "5.5.0",
        "cross-env": "7.0.2",
        "enquirer": "2.3.6",
        "fs-extra": "9.0.1",
        "graphql": "15.1.0",
        "lodash": "4.17.20",
        "node-fetch": "2.6.1",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.3",
        "semver": "7.3.2",
        "tslib": "1.10.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "1.22.2",
        "@oclif/test": "1.2.7",
        "@types/fs-extra": "8.1.0",
        "@types/jest": "26.0.7",
        "@types/lodash": "4.14.149",
        "@types/node": "10.17.13",
        "@types/node-fetch": "2.5.7",
        "@types/semver": "7.3.1",
        "chai": "4.2.0",
        "globby": "10.0.2",
        "jest": "26.1.0",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.1",
        "ts-jest": "26.1.3",
        "ts-node": "8.6.2",
        "typescript": "3.9.7"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "keywords": [
        "oclif"
    ],
    "oclif": {
        "commands": "./dist/commands",
        "topics": {
            "app": {
                "description": "Manage Graph Apps"
            },
            "db": {
                "description": "Manage Neo4j Databases"
            },
            "dbms": {
                "description": "Manage Neo4j DBMSs"
            },
            "environment": {
                "description": "Manage relate environments"
            },
            "extension": {
                "description": "Manage relate extensions"
            },
            "project": {
                "description": "Manage relate projects"
            }
        },
        "hooks": {
            "command_not_found": "./dist/hooks/command-not-found"
        },
        "bin": "relate",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-autocomplete"
        ]
    },
    "scripts": {
        "dev-cli": "cross-env NODE_PATH=./node_modules oclif-dev",
        "prepare": "npm run build",
        "postpack": "rimraf oclif.manifest.json",
        "prepack": "run-s build:clean build:manifest generate:docs",
        "build": "npm run prepack",
        "build:clean": "run-s clean build:tsc",
        "build:tsc": "tsc -b",
        "build:manifest": "node -e '' | npm run dev-cli -- manifest",
        "generate:docs": "npm run dev-cli -- readme --multi --dir=./docs",
        "clean": "rimraf dist",
        "test": "jest",
        "add:docs": "git add README.md docs",
        "version": "run-s generate:docs add:docs",
        "watch": "tsc -w"
    }
}
