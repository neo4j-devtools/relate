{
    "inputFiles": ["./src/entities"],
    "exclude": ["!./src/entities/**/*.abstract.ts"],
    "mode": "file",
    "out": "documentation",
    "excludePrivate": true,
    "excludeProtected": true,
    "gitRevision": "master",
    "gitRemote": "upstream",
    "readme": "none",
    "plugins": [
        "typedoc-plugin-no-inherit",
        "typedoc-plugin-markdown"
    ]
}
