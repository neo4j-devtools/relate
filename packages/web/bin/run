#!/usr/bin/env node
const childProcess = require('child_process');
const path = require('path');

const NODE_PATH = path.join(__dirname, '../node_modules');

childProcess.fork(
    path.join(__dirname, '../dist/dev.js'),
    process.argv.slice(2),
    {
        stdio: 'inherit',
        env: {NODE_PATH}
    }
)
