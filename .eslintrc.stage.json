{
    "extends": "./.eslintrc.json",
    "rules": {
        "no-console": "error",
        "no-constant-condition": "error",
        "no-debugger": "error",
        "no-unreachable": "error",
        "no-magic-numbers": "error",
        "no-empty-function": [
            "error",
            {
                "allow": ["constructors"]
            }
        ],
        "require-await": "error",
        "no-lonely-if": "error",
        "no-multiple-empty-lines": "error"
    }
}
